    "keywords": [
        "wordpress",
        "hizzle",
        "store"
    ],
    "homepage": "http://github.com/hizzle-co/datastore/",
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Brian Mutende",
            "email": "picocodes@gmail.com",
            "homepage": "http://github.com/picocodes/",
            "role": "Developer"
        }
