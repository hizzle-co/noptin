{
	"name": "noptin",
	"version": "1.6.0",
	"private": true,
	"description": "WordPress Newsletter Plugin",
	"scripts": {
		"test": "test",
		"cssform": "postcss includes/assets/css/postcss/form-styles.css -o includes/assets/css/form-styles.css",
		"cssa": "postcss includes/assets/css/postcss/admin.css -o includes/assets/css/admin.css",
		"cssf": "postcss includes/assets/css/postcss/frontend.css -o includes/assets/css/frontend.css",
		"csshelper": "postcss includes/assets/css/postcss/noptin-helper.css -o includes/assets/css/noptin-helper.css",
		"cssaddons": "postcss includes/assets/css/postcss/addons-page.css -o includes/assets/css/addons-page.css",
		"css": "npm run cssa && npm run cssf && npm run csshelper && npm run cssaddons",
		"js": "npx webpack --watch"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hizzle-co/noptin.git"
